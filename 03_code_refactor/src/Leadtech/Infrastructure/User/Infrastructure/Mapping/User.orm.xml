<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Leadtech\Domain\Entity\User" table="customer" schema="Leadtech" repository-class="Leadtech\Infrastructure\Repository\MysqlRepository\UserRepository">
        <id name="id" type="integer" column="id" >
            <generator strategy="AUTO" />
        </id>
        <field name="name" type="string" length="150" />
        <field name="email" type="string" length="150" />
    </entity>
</doctrine-mapping>